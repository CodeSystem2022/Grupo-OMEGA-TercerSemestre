package accesodatos;


public interface IAccesoDatos {
    int MAX_REGISTRO = 10;
    //Metodo insert es abstracto y sin cuerpo 
    void insertar();//esto es sin cuerpo, no habrimos llave ni nada dentro del cuerpo
    
    void listar();
    
    void actualizar();
    
    void eliminar();
    
}
